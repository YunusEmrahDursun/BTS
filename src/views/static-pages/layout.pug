doctype html
html(lang='en')
  head
    title=title+" | BTS" 
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible' content='IE=edge')
    meta(name='viewport' content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0')
    meta(name='description' content='Oculux Bootstrap 4x admin is super flexible, powerful, clean & modern responsive admin dashboard with unlimited possibilities.')
    meta(name='keywords' content='admin template, Oculux admin template, dashboard template, flat admin template, responsive admin template, web app, Light Dark version')
    meta(name='author' content='GetBootstrap, design by: puffintheme.com')
    link(rel='icon' href='/images/favicon.ico' type='image/x-icon')
    // VENDOR CSS
    link(rel='stylesheet' href='/css/bootstrap.min.css')
    link(rel='stylesheet' href='/css/font-awesome.min.css')
    link(rel='stylesheet' href='/css/vivify.min.css')
    link(rel='stylesheet' href='/css/c3.min.css')
    link(rel='stylesheet' href='/css/jquery-confirm.min.css')
    // MAIN CSS
    link(rel='stylesheet' href='/css/site.min.css')
    link(rel='stylesheet' href='/css/style.css')

    link(rel='stylesheet' href='/css/toastr.min.css')
    link(rel='stylesheet' href='/css/table-form.css')
    block style
  body.theme-cyan.font-montserrat.light_version
    // Page Loader
    .page-loader-wrapper
      .loader
        .bar1
        .bar2
        .bar3
        .bar4
        .bar5
    // Overlay For Sidebars
    .overlay
    #wrapper
      include header
      #megamenu.megamenu.particles_js
        a.megamenu_toggle.btn.btn-danger(href='javascript:void(0);')
          i.icon-close
        .container
          .row.clearfix
            .col-12
              ul.q_links
                li
                  a(href='app-inbox.html')
                    i.icon-envelope
                    span Inbox
                li
                  a(href='app-chat.html')
                    i.icon-bubbles
                    span Messenger
                li
                  a(href='app-calendar.html')
                    i.icon-calendar
                    span Event
                li
                  a(href='page-profile.html')
                    i.icon-user
                    span Profile
                li
                  a(href='page-invoices.html')
                    i.icon-printer
                    span Invoice
                li
                  a(href='page-timeline.html')
                    i.icon-list
                    span Timeline
            .col-md-4.col-sm-12
              .card.w_card3
                .body
                  .text-center
                    i.icon-picture.text-info
                    h4.m-t-25.mb-0 104 Picture
                    p Your gallery download complete
                    a.btn.btn-info.btn-round(href='javascript:void(0);') Download now
            .col-md-4.col-sm-12
              .card.w_card3
                .body
                  .text-center
                    i.icon-diamond.text-success
                    h4.m-t-25.mb-0 813 Point
                    p You are doing great job!
                    a.btn.btn-success.btn-round(href='javascript:void(0);') Read now
            .col-md-4.col-sm-12
              .card.w_card3
                .body
                  .text-center
                    i.icon-social-twitter.text-primary
                    h4.m-t-25.mb-0 3,756
                    p New Followers on Twitter
                    a.btn.btn-primary.btn-round(href='javascript:void(0);') Find more
            .col-12
              ul.list-group
                li.list-group-item
                  | Anyone send me a message
                  .float-right
                    label.switch
                      input(type='checkbox' checked='')
                      span.slider.round
                li.list-group-item
                  | Anyone seeing my profile page
                  .float-right
                    label.switch
                      input(type='checkbox' checked='')
                      span.slider.round
                li.list-group-item
                  | Anyone posts a comment on my post
                  .float-right
                    label.switch
                      input(type='checkbox')
                      span.slider.round
        #particles-js
      include sidebar                  

    #main-content
      .container-fluid
        block content
    // Javascript
    script(src='/bundles/libscripts.bundle.js')
    link(rel='stylesheet' href='/css/select2.css')
    link(rel='stylesheet' href='/css/parsley.css')
    link(rel='stylesheet' href='/css/dropify.min.css')
    script(src='/js/parsley.min.js')
    script(src='/js/select2.js')  
    script(src='/bundles/vendorscripts.bundle.js')
    script(src='/bundles/moment.min.js')
    script(src='/bundles/moment.tr.min.js')
    script(src='/bundles/c3.bundle.js')
    script(src='/bundles/mainscripts.bundle.js')
    script(src='/js/toastr.js')
    script(src='/js/jquery.inputmask.bundle.js')
    script(src='/js/main.js')
    script(src='/scripts/jquery-confirm.min.js')
    script(src='/scripts/http.js')
    script(src='/scripts/socket.io.min.js')
    script(src='/scripts/socket.js')
    include ../components/componentjs
    block javascript